{
  "name": "tauria-challenge",
  "version": "1.0.0",
  "description": "Basic setup for Node, Express and Typescript projects",
  "main": "index.js",
  "scripts": {
    "install:server": "npm i",
    "install:client": "cd client && npm i",
    "start:server": "nodemon --exec ts-node index.ts",
    "start:client": "npm start --prefix client",
    "start": "concurrently \"npm run start:*\" ",
    "build:server": "tsc index.ts",
    "build:client": "npm run build --prefix client",
    "build": "concurrently \"npm run build:*\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsassiv/tauria-challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fsassiv/tauria-challenge/issues"
  },
  "homepage": "https://github.com/fsassiv/tauria-challenge#readme",
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0"
  }
}
